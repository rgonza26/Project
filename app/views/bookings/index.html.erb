<h1>Bookings</h1>
    <table class="table table-hover">
      <thead>
        <tr>
          <div class="container">
            <ul class="nav events">
              <th>UserID</th>
              <th>EventID</th>
              <th colspan="3"></th>
        </tr>
      </thead>
	  <tbody>
	        <% @bookings.each do |booking| %>
